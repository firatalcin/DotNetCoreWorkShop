@model List<Category>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List</title>
</head>
<body>

    <h2 style="text-align:center">Kategori Listesi</h2>

    <ul>
        @foreach (var item in Model)
       {
              <li>@item.CategoryName</li>
       }     
    </ul>
</body>
</html>
